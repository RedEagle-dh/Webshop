CREATE TABLE `description_mb` (
  `Mainboard Modell:` varchar(45) DEFAULT NULL,
  `Mainboard Sockel:` varchar(45) DEFAULT NULL,
  `Mainboard Chipsatz:` varchar(45) DEFAULT NULL,
  `Mainboard Formfaktor:` varchar(45) DEFAULT NULL,
  `Onboard Grafik:` varchar(45) DEFAULT NULL,
  `Grafikausgänge:` varchar(45) DEFAULT NULL,
  `MultiGPU Fähigkeit:` varchar(45) DEFAULT NULL,
  `Arbeitsspeicher Slots:` varchar(45) DEFAULT NULL,
  `Arbeitsspeicher Typ:` varchar(45) DEFAULT NULL,
  `Arbeitsspeicher Bauform:` varchar(45) DEFAULT NULL,
  `Arbeitsspeicherarchitektur:` varchar(45) DEFAULT NULL,
  `Max. Kapazität der Einzelmodule:` varchar(45) DEFAULT NULL,
  `Unterstützte Speichermodule:` varchar(500) DEFAULT NULL,
  `Anzahl PCIe x16 Slots:` varchar(45) DEFAULT NULL,
  `Anzahl PCIe x8 Slots:` varchar(45) DEFAULT NULL,
  `Anzahl PCIe x4 Slots:` varchar(45) DEFAULT NULL,
  `Anzahl PCIe Slots:` varchar(45) DEFAULT NULL,
  `Anzahl PCI-X Slots:` varchar(45) DEFAULT NULL,
  `Anzahl PCI Slots:` varchar(45) DEFAULT NULL,
  `Anzahl AGP Slots:` varchar(45) DEFAULT NULL,
  `Anzahl mSATA Anschlüsse:` varchar(45) DEFAULT NULL,
  `Anzahl M.2 Anschlüsse:` varchar(45) DEFAULT NULL,
  `Anzahl SATA 6GB/s Anschlüsse:` varchar(45) DEFAULT NULL,
  `Anzahl SATA 3GB/s Anschlüsse:` varchar(45) DEFAULT NULL,
  `Anzahl SAS 12GB/s Anschlüsse:` varchar(45) DEFAULT NULL,
  `Anzahl SAS 6GB/s Anschlüsse:` varchar(45) DEFAULT NULL,
  `Anzahl IDE Anschlüsse:` varchar(45) DEFAULT NULL,
  `Anzahl SAS 3GB/s Anschlüsse:` varchar(45) DEFAULT NULL,
  `Anzahl Floppy Anschlüsse:` varchar(45) DEFAULT NULL,
  `Soundcodec:` varchar(45) DEFAULT NULL,
  `Audiotyp:` varchar(45) DEFAULT NULL,
  `Audioanschlüsse:` varchar(45) DEFAULT NULL,
  `netzwerkadapter:` varchar(45) DEFAULT NULL,
  `Wireless Lan:` varchar(45) DEFAULT NULL,
  `Bluetooth:` varchar(45) DEFAULT NULL,
  `Anzahl USB2.0 Anschlüsse:` varchar(45) DEFAULT NULL,
  `Anzahl USB3.2 Gen 1 Anschlüsse (USB 3.0):` varchar(45) DEFAULT NULL,
  `Anzahl USB3.2 Gen 2 Anschlüsse (USB 3.1):` varchar(45) DEFAULT NULL,
  `Anzahl FireWire Anschlüsse:` varchar(45) DEFAULT NULL,
  `Anzahl USB 3.2 Gen 2x2 Anschlüsse:` varchar(45) DEFAULT NULL,
  `Anzahl Parallelport Anschlüsse:` varchar(45) DEFAULT NULL,
  `Anzahl Seriellport Anschlüsse:` varchar(45) DEFAULT NULL,
  `Anzahl eSATA Anschlüsse:` varchar(45) DEFAULT NULL,
  `Anzahl PS/2 Anschlüsse:` varchar(45) DEFAULT NULL,
  `BIOS Typ:` varchar(45) DEFAULT NULL,
  `integrierte CPU:` varchar(45) DEFAULT NULL,
  `Verpackung:` varchar(45) DEFAULT NULL,
  `productid` int DEFAULT NULL,
  `id` int NOT NULL,
  PRIMARY KEY (`id`),
  UNIQUE KEY `productid_UNIQUE` (`productid`),
  CONSTRAINT `description_mb_pid` FOREIGN KEY (`productid`) REFERENCES `produkte` (`artnr`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci;




CREATE TABLE `description_gpu` (
  `GPU Modell:` varchar(45) DEFAULT NULL,
  `Edition:` varchar(45) DEFAULT NULL,
  `Schnittstelle:` varchar(45) DEFAULT NULL,
  `GPU Anzahl:` varchar(45) DEFAULT NULL,
  `GPU Takt:` varchar(45) DEFAULT NULL,
  `Shader Model:` varchar(45) DEFAULT NULL,
  `Anzahl der Streamprozessoren:` varchar(45) DEFAULT NULL,
  `Fertigungsprozess:` varchar(45) DEFAULT NULL,
  `Grösse des Grafikspeichers:` varchar(45) DEFAULT NULL,
  `Grafikspeichertyp:` varchar(45) DEFAULT NULL,
  `Grafikspeicher Taktfrequenz:` varchar(45) DEFAULT NULL,
  `Grafikspeicher Anbindung:` varchar(45) DEFAULT NULL,
  `Anschlüsse der Grafikkarte:` varchar(45) DEFAULT NULL,
  `Direct X Version:` varchar(45) DEFAULT NULL,
  `OpenGL Version:` varchar(45) DEFAULT NULL,
  `Kühlung der Grafikkarte:` varchar(45) DEFAULT NULL,
  `Stromversorgung:` varchar(45) DEFAULT NULL,
  `Max. Stromverbrauch:` varchar(45) DEFAULT NULL,
  `Grafikkarten Bauform:` varchar(45) DEFAULT NULL,
  `Verpackung:` varchar(45) DEFAULT NULL,
  `Besonderheiten` varchar(250) DEFAULT NULL,
  `productid` int DEFAULT NULL,
  `id` int NOT NULL,
  PRIMARY KEY (`id`),
  UNIQUE KEY `productid_UNIQUE` (`productid`),
  CONSTRAINT `description_gpu_pid` FOREIGN KEY (`productid`) REFERENCES `produkte` (`artnr`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci;


CREATE TABLE `description_cpu` (
  `Prozessor Serie:` varchar(45) DEFAULT NULL,
  `Prozessor Modell:` varchar(45) DEFAULT NULL,
  `Codename:` varchar(45) DEFAULT NULL,
  `Anzahl der CPU Kerne:` varchar(45) DEFAULT NULL,
  `Prozessortakt:` varchar(45) DEFAULT NULL,
  `Max. Turbotakt:` varchar(45) DEFAULT NULL,
  `Sockel:` varchar(45) DEFAULT NULL,
  `Fertigungsprozess:` varchar(45) DEFAULT NULL,
  `TDP:` varchar(45) DEFAULT NULL,
  `DMI Takt:` varchar(45) DEFAULT NULL,
  `L2 Cache:` varchar(45) DEFAULT NULL,
  `L3 Cache:` varchar(45) DEFAULT NULL,
  `Integrierte Grafik:` varchar(45) DEFAULT NULL,
  `Besonderheiten:` varchar(250) DEFAULT NULL,
  `Verpackungsart des Prozessors:` varchar(45) DEFAULT NULL,
  `Anzahl der Threads:` varchar(45) DEFAULT NULL,
  `productid` int DEFAULT NULL,
  `id` int NOT NULL,
  PRIMARY KEY (`id`),
  UNIQUE KEY `productid_UNIQUE` (`productid`),
  CONSTRAINT `techdata_pid` FOREIGN KEY (`productid`) REFERENCES `produkte` (`artnr`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci;